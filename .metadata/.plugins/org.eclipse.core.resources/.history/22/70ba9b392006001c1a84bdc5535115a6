package com.cognixia.jump.bridgeCourse.classes;

public class StringDemo {
	
	public static void main(String[] args) {
		
		Dog dog = new Dog(); 
		Dog dog2 = new Dog();
		System.out.println(dog.hashCode() + " " + dog2.hashCode());
		System.out.println(dog + "\n" + dog2);
		
		
		//String Pool - what happens in memory 
		String s1 = "s1";
		
		String s2 = "s1";
		
		String s3 = "s3";
		
		String s4 = new String("s1");
		
		
		//String - object/instance methods 
		
		
		
		//String - class/static methods 
		
		
		
		
	}

}
